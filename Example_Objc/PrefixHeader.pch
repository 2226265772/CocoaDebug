#import "CocoaDebugObjcLog.h"

#pragma clang diagnostic ignored "-Wunused-value"


//----------------- white log (default) -----------------
#ifdef DEBUG
#define NSLog(fmt, ...) [CocoaDebugObjcLog logWithFile:__FILE__ function:NSStringFromSelector(_cmd) line:__LINE__ color:[UIColor whiteColor] message:(fmt), ##__VA_ARGS__]
#else
#define NSLog(fmt, ...) nil
#endif


//----------------- red log -----------------
#ifdef DEBUG
#define NSLogRed(fmt, ...) [CocoaDebugObjcLog logWithFile:__FILE__ function:NSStringFromSelector(_cmd) line:__LINE__ color:[UIColor redColor] message:(fmt), ##__VA_ARGS__]
#else
#define NSLog(fmt, ...) nil
#endif


//----------------- blue log -----------------
#ifdef DEBUG
#define NSLogBlue(fmt, ...) [CocoaDebugObjcLog logWithFile:__FILE__ function:NSStringFromSelector(_cmd) line:__LINE__ color:[UIColor blueColor] message:(fmt), ##__VA_ARGS__]
#else
#define NSLog(fmt, ...) nil
#endif


//----------------- yellow log -----------------
#ifdef DEBUG
#define NSLogYellow(fmt, ...) [CocoaDebugObjcLog logWithFile:__FILE__ function:NSStringFromSelector(_cmd) line:__LINE__ color:[UIColor yellowColor] message:(fmt), ##__VA_ARGS__]
#else
#define NSLog(fmt, ...) nil
#endif


