#import "Example_Objc-Swift.h"
#import "CocoaDebug+Extension.h"

#pragma clang diagnostic ignored "-Wunused-value"


//----------------- white log (default) -----------------
#ifdef DEBUG
#define CocoaLog(fmt, ...) [CocoaDebug objcLogWithFile:__FILE__ function:NSStringFromSelector(_cmd) line:__LINE__ color:[UIColor whiteColor] message:(fmt), ##__VA_ARGS__]
#else
#define NSLog(fmt, ...) nil
#endif


//----------------- red log -----------------
#ifdef DEBUG
#define CocoaLogRed(fmt, ...) [CocoaDebug objcLogWithFile:__FILE__ function:NSStringFromSelector(_cmd) line:__LINE__ color:[UIColor redColor] message:(fmt), ##__VA_ARGS__]
#else
#define NSLog(fmt, ...) nil
#endif


//----------------- blue log -----------------
#ifdef DEBUG
#define CocoaLogBlue(fmt, ...) [CocoaDebug objcLogWithFile:__FILE__ function:NSStringFromSelector(_cmd) line:__LINE__ color:[UIColor blueColor] message:(fmt), ##__VA_ARGS__]
#else
#define NSLog(fmt, ...) nil
#endif


//----------------- yellow log -----------------
#ifdef DEBUG
#define CocoaLogYellow(fmt, ...) [CocoaDebug objcLogWithFile:__FILE__ function:NSStringFromSelector(_cmd) line:__LINE__ color:[UIColor yellowColor] message:(fmt), ##__VA_ARGS__]
#else
#define NSLog(fmt, ...) nil
#endif


